{
  "name": "karma-typescript-coverage-issue",
  "version": "1.0.0",
  "description": "Karma Typescript Coverage Issue",
  "scripts": {
    "clean": "npm run clean-dist && npm run clean-coverage",
    "clean-coverage": "./node_modules/.bin/rimraf \"./coverage\"",
